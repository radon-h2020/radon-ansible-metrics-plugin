{
	"name": "ansiblemetrics",
	"displayName": "AnsibleMetrics",
	"description": "An extension of AnsibleMetrics to quantify the characteristics of an Ansible blueprints.",
	"version": "0.0.1",
	"publisher": "sdallapalma",
	"url": "https://sdallapalma@dev.azure.com/sdallapalma/radon-defect-prediction-plugin/_git/radon-defect-prediction-plugin",
	"engines": {
		"vscode": "^1.45.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/radon-h2020/radon-ansible-metrics-plugin.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ansiblemetrics.run"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ansiblemetrics.run",
				"title": "AnsibleMetrics"
			}
		],
		"menus": {
			"editor/title": [
			  {
				"when": "resourceLangId == yaml",
				"command": "ansiblemetrics.run",
				"group": "navigation"
			  }
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
